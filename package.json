{
    "author": "askr3d",
    "devDependencies": {
        "@rollup/plugin-image": "^3.0.3",
        "@size-limit/preset-small-lib": "^11.1.6",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "husky": "^9.1.7",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "size-limit": "^11.1.6",
        "tsdx": "^0.14.1",
        "tslib": "^2.8.1",
        "typescript": "^3.9.10"
    },
    "engines": {
        "node": ">=10"
    },
    "files": [
        "dist",
        "src"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/askred-product-card.esm.js",
    "name": "askred-product-card",
    "peerDependencies": {
        "react": ">=16"
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "scripts": {
        "analyze": "size-limit --why",
        "build": "tsdx build",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "start": "tsdx watch",
        "test": "tsdx test --passWithNoTests"
    },
    "size-limit": [
        {
            "limit": "10 KB",
            "path": "dist/askred-product-card.cjs.production.min.js"
        },
        {
            "limit": "10 KB",
            "path": "dist/askred-product-card.esm.js"
        }
    ],
    "typings": "dist/index.d.ts",
    "version": "0.1.0"
}